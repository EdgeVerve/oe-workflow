{
  "name": "bpmndata",
  "base": "BaseEntity",
  "description": "Contains XML data of BPMN workflows",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "versionmessage": {
      "type": "string"
    },
    "bpmnname": {
      "type": "string",
      "required": true
    },
    "xmldata": {
      "type": "string",
      "required": true,
      "oracle": {
        "dataType": "CLOB"
      }
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {
    "processDefinitions": {
      "type": "hasMany",
      "model": "ProcessDefinition"
    },
    "workflowDefinition": {
      "type": "hasOne",
      "model": "WorkflowDefinition"
    }
  }
}